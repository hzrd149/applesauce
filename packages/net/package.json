{
  "name": "applesauce-net",
  "version": "0.9.0",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch:build": "tsc --watch > /dev/null",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "watch:test": "(trap 'kill 0' SIGINT; pnpm run build -w > /dev/null & pnpm run test --watch)"
  },
  "keywords": [
    "nostr"
  ],
  "author": "hzrd149",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./connection": {
      "import": "./dist/connection/index.js",
      "types": "./dist/connection/index.d.ts"
    },
    "./connection/*": {
      "import": "./dist/connection/*.js",
      "types": "./dist/connection/*.d.ts"
    },
    "./subscription": {
      "import": "./dist/subscription/index.js",
      "types": "./dist/subscription/index.d.ts"
    },
    "./subscription/*": {
      "import": "./dist/subscription/*.js",
      "types": "./dist/subscription/*.d.ts"
    }
  },
  "dependencies": {
    "applesauce-core": "^0.9.0",
    "nanoid": "^5.0.7",
    "nostr-tools": "^2.10.3",
    "rxjs": "^7.8.1",
    "zen-push": "^0.3.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.13",
    "@types/zen-push": "^0.1.4",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "typescript": "^5.6.3"
  },
  "jest": {
    "roots": [
      "dist"
    ],
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ]
  },
  "funding": {
    "type": "lightning",
    "url": "lightning:nostrudel@geyser.fund"
  }
}
